{
  "name": "sort-vision",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "test": "node tests/quality-assurance.mjs",
    "test:quick": "node tests/quality-assurance.mjs --quick",
    "test:prod": "node tests/quality-assurance.mjs --production",
    "test:extended": "node tests/quality-assurance.mjs --extended",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,yml,yaml}\"",
    "preview": "next start",
    "generate-og": "node scripts/generate-og-image.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "submit-indexnow": "node scripts/submit-indexnow.mjs --sitemap",
    "test-sitemap": "node scripts/test-sitemap-urls.js",
    "test-sitemap-bash": "bash scripts/test-sitemap-urls.sh",
    "validate-seo": "npm run generate-sitemap && npm run test-sitemap && npm run submit-indexnow",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@surma/rollup-plugin-off-main-thread": "^2.2.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "braces": "^3.0.3",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "ejs": "^4.0.1",
    "filelist": "^1.0.4",
    "framer-motion": "^12.33.0",
    "http-proxy-middleware": "^3.0.5",
    "jake": "^10.9.4",
    "lucide-react": "^0.563.0",
    "micromatch": "^4.0.8",
    "minimatch": "^10.1.2",
    "next": "^16.1.6",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^7.0.2",
    "on-headers": "^1.1.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-ga4": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/plugin-kit": "^0.6.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "1.0.0",
    "baseline-browser-mapping": "^2.9.19",
    "compression": "^1.8.1",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "express": "^5.2.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.27.2",
      "minimatch": "^10.1.1",
      "braces": "^3.0.3",
      "react-helmet-async>react": "^19.2.3",
      "react-helmet-async>react-dom": "^19.2.3"
    },
    "ignoredBuiltDependencies": [
      "@vercel/speed-insights"
    ],
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,json,css,md,yml,yaml}": [
      "git add"
    ]
  },
  "packageManager": "pnpm@10.28.2"
}
